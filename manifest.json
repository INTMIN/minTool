{
  "manifest_version": 3,
  "name": "cos二次元人物",
  "description": "能在网页中插入cos二次元人物",
  "version": "1.2.1",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["live2d.js", "wanfu.js", "content.js"],
      "css": ["wanfu.css"]
    }
  ],
  "permissions": ["storage", "tabs", "activeTab", "scripting"]
}
