{
  "manifest_version": 2,
  "name": "cos二次元人物",
  "description": "能在网页中插入cos二次元人物",
  "version": "1.0",
  "action": {
    "popup": "cos.html"
  },

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["live2d.js", "wanfu.js", "content.js"],
      "css": ["wanfu.css"]
    }
  ],
  "permissions": ["storage", "tabs", "activeTab", "https://*/*", "scripting"]
}
